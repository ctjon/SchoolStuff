SUB Text3_KeyPress (KeyAscii AS INTEGER)
    STATIC Temp AS STRING
    DIM CharIn AS STRING
    DIM Mon AS STRING
    DIM Daay AS STRING
    DIM Yr AS STRING

    IF (LEN(Temp) > 5) THEN
        KeyAscii = 0
        BEEP
    ELSEIF (KeyAscii >= 48 AND KeyAscii <= 57) OR (KeyAscii = 47) THEN
        CharIn = CHR$(KeyAscii)
        Temp = Temp + CharIn
        Text3.Text = Temp
        KeyAscii = 0
        Tag = Tag + CharIn
    ELSE
        msg$ = "Invalid Date"
        MSGBOX msg$
        KeyAscii = 0
        BEEP
    END IF
    IF LEN(Temp) = 5 THEN
        Mon = LEFT$(Temp, 2)
        Daay = MID$(Temp, 2, 2)
        Yr = RIGHT$(Temp, 2)
        SELECT CASE Mon
            CASE "01", "03", "05", "07", "08", "10", "12"
            IF (VAL(Daay) < 1) OR (VAL(Daay) >= 31) THEN
                msg$ = "Invalid Date"
                MSGBOX msg$
                Temp = ""
                Text3.Text = ""
            END IF
            CASE "04", "06", "09", "11"
            IF (VAL(Daay) < 1) OR (VAL(Daay) >= 30) THEN
                msg$ = "Invalid Date"
                MSGBOX msg$
                Temp = ""
                Text3.Text = ""
            END IF
            CASE "02"
            IF (VAL(Yr) = 0) OR (VAL(Yr) MOD 4 <> 0) THEN
                IF (VAL(Daay) < 1) OR (VAL(Daay) >= 28) THEN
                    msg$ = "Invalid Date"
                    MSGBOX msg$
                    Temp = ""
                    Text3.Text = ""
                ELSEIF (VAL(Daay) < 1) OR (VAL(Daay) >= 29) THEN
                    msg$ = "Invalid Date"
                    MSGBOX msg$
                    Temp = ""
                    Text3.Text = ""
                END IF
            END IF
            CASE ELSE
                msg$ = "Invalid Date"
                MSGBOX msg$
                Temp = ""
                Text3.Text = ""
        END SELECT
    END IF
END SUB

